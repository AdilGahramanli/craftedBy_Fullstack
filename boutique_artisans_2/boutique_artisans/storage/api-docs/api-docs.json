{
    "openapi": "3.0.0",
    "info": {
        "title": "Crafted By API",
        "description": "All the routes for the Crafted By shop.",
        "contact": {
            "name": "Crafted By Team",
            "email": "maxime@dysign.fr"
        },
        "version": "1.0.0"
    },
    "servers": [
        {
            "url": "https://api.gahramanli.devlog25.le-campus.eu/api",
            "description": "API Server"
        }
    ],
    "paths": {
        "/products": {
            "get": {
                "tags": [
                    "Products"
                ],
                "summary": "Get list of all products",
                "description": "Returns list of products",
                "operationId": "c1ff6f862214e1896c59cfe0491ce0e8",
                "responses": {
                    "200": {
                        "description": "Successful operation"
                    }
                }
            }
        },
        "/products/{id}": {
            "get": {
                "tags": [
                    "Products"
                ],
                "summary": "Get product details by ID",
                "description": "Returns details of a specific product by ID.",
                "operationId": "getProductById",
                "parameters": [
                    {
                        "name": "id",
                        "in": "path",
                        "description": "ID of the product to retrieve",
                        "required": true,
                        "schema": {
                            "type": "string",
                            "example": "9x508988-2d3g-44f6-ac72-0f93eac22rrd"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Successful operation",
                        "content": {
                            "text/html": {}
                        }
                    },
                    "404": {
                        "description": "Product not found"
                    }
                }
            }
        },
        "/shops": {
            "get": {
                "tags": [
                    "Shops"
                ],
                "summary": "Get list of all shops",
                "description": "Returns a list of all shops.",
                "operationId": "getShops",
                "responses": {
                    "200": {
                        "description": "Successful operation",
                        "content": {
                            "text/html": {}
                        }
                    }
                }
            }
        }
    }
}
